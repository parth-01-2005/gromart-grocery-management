{
    "version": 2,
    "functions": {
      "api/**/*.js": {
        "runtime": "nodejs18.x"
      },
      "server/**/*.js": {
        "runtime": "nodejs18.x"
      }
    },
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "/server/$1"
      }
    ]
  }